    BEGIN
	  INTEGER L;
	  INTEGER i;
	  INTEGER j;
	  INTEGER k;
      L:=12;
      i:=1;
      j:=0;
      FOR j := 1 STEP 1 UNTIL 5 DO
      BEGIN
      outinteger(1,L);
      L:= L+3;
      IF (j / 15 EQUAL 1) THEN i:=2
      END;
      L:=L*3;
      outinteger(1,L);
      L:=L+3;
      outinteger(1,L);
      L:=L-3;
      outinteger(1,L);
      L:=L/2;
      outinteger(1,L);
      outinteger(1,i)
    END




	begin
	   integer i;
	   integer count;
	   integer procedure A (k, x1, x2, x3, x4, x5);
	   value k; integer k, x1, x2, x3, x4, x5;
	   begin
	      integer procedure B;
	      begin
	         k := k - 1;
	      end;
		count := count + 1;
	      if k <= 0 then A := x4 + x5 else B;
	   end;
	   count := 0;
	   for i := 1 step 1 until 15 do
	   begin
	      count := 0;
	   end;
	end;

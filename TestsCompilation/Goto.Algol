    BEGIN
	  INTEGER L;
	  INTEGER i;
	  INTEGER k;
      TESTLABEL :
      	BEGIN
      	     IF (i LESS 3) THEN L:=14 ELSE L:=12;
      	     k:=4;
        	  outinteger(1,L)
        END;
        Wawa :
        BEGIN
        FOR i := 1 STEP 1 UNTIL 5 DO
        BEGIN
        outinteger(1,L)
        END
        END
        ;
      L:=12;
      i:=1;
      outinteger(1,L);
      GOTO TESTLABEL;
      GOTO Wawa
    END
